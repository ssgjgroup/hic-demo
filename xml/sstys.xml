    <node id="e74839a4-0403-4f10-ace3-d8afc91ecab0" nodetype="DynamicMoleNode" nodelogiccontent="" name="手术中可能出现的意外及风险"
          signid="手术中可能出现的意外及风险|419" text="手术中可能出现的意外及风险" description="有创检查知情同意书项目" otherid="C-1192"
          display="10``&#xD;&#xA;`0`1" permitobjs="" defaultobjs="" allowUserDefine="True" canNotInputText="False"
