        select count(1) from HLHT_ZYBCJL_SQXJ t
        <where>
            <include refid="sql-hlhtZybcjlSqxj"/>
        </where>
    </select>

    <select id="selectHlhtZybcjlSqxjPageList" resultMap="hlhtZybcjlSqxjResult"
            parameterType="com.winning.hic.model.HlhtZybcjlSqxj">
        SELECT * FROM (
        SELECT t.* ,
       ROW_NUMBER() OVER ( ORDER BY t.yjlxh DESC ) AS _RN
        FROM HLHT_ZYBCJL_SQXJ t
        <where>
            <include refid="sql-hlhtZybcjlSqxj"/>
        </where>
        <!-- order by  asc -->
        ) TEMP_TABLE WHERE _RN BETWEEN #{row.first} + 1 AND #{row.first} + #{row.count}
