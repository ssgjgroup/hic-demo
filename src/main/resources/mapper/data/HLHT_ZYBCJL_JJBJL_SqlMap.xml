    <select id="selectInitialHlhtZybcjlJjbjl" resultMap="hlhtZybcjlJjbjlResult"
            parameterType="com.winning.hic.model.HlhtZybcjlJjbjl">
        SELECT t.QTBLJLXH as yjlxh,b.HISSYXH as jzlsh,c.PATID as patid,
        b.ZYHM as zyh,c.KSDM as ksdm,c.KSMC as ksmc,c.BQDM as bqdm,
        c.BQMC as bqmc ,a.fjh as bfh ,NULL as bfmc ,c.CWDM as bch ,b.HZXM as hzxm,
        b.SFZH as sfzhm,b.BRXB as xbdm,c.SEX as xbmc,
        ISNULL(convert (varchar,(YEAR(GETDATE())-YEAR(convert(datetime, b.CSRQ)))) ,'0') AS nls,
        DATEDIFF(MONTH,b.CSRQ,SUBSTRING(CONVERT(CHAR(8),GETDATE(),112),1,8)) %12 AS nly,c.RYRQ as ryrq
        FROM CISDB.dbo.EMR_QTBLJLK t
        LEFT JOIN CISDB.dbo.CPOE_BRSYK c ON t.SYXH = c.EMRXH
        LEFT JOIN CISDB.dbo.EMR_BRSYK b ON b.SYXH = t.SYXH
        LEFT JOIN THIS4.dbo.ZY_BCDMK a ON a.id = b.RYCW and a.bqdm=b.RYBQ
        WHERE 1=1
