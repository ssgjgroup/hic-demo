        <result column="NODE_PID" property="nodePid" jdbcType="BIGINT" />
        <result column="NODE_TEXT" property="text" jdbcType="VARCHAR" />
        <result column="PY_CODE" property="pyCode" jdbcType="VARCHAR" />
