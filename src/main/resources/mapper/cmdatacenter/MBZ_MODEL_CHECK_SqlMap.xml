            <if test="(yzjddm != null and yzjddm != '')">#{yzjddm , jdbcType= VARCHAR },</if>
            <if test="(bt != null and bt != '')">#{bt , jdbcType= INTEGER },</if>
            <if test="(mustMatch != null)">#{mustMatch , jdbcType= INTEGER },</if>
