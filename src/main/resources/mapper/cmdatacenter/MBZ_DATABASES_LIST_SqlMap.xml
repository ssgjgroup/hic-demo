            <if test="(dataAlias != null and dataAlias != '')"> #{dataAlias , jdbcType= VARCHAR },  </if>
            <if test="(ip != null and ip != '')"> #{ip , jdbcType= VARCHAR },  </if>
            <if test="(userName != null and userName != '')"> #{userName , jdbcType= VARCHAR },  </if>
