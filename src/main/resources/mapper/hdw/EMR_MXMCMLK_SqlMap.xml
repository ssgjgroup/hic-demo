        SELECT
               a.mldm,a.mlmc,left(a.mldm,len(a.mldm)-2) sjdm,
               (SELECT COUNT(1) FROM EMR_MBK c
                 LEFT JOIN EMR_KSMBDY b on c.mbdm = b.mbdm where c.mxfldm =a.mldm and (c.grmbbz = 0 or c.grmbbz is null) and b.ksdm is not null ) NUM
