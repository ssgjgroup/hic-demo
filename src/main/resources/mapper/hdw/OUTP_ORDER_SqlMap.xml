    <delete id="deleteOutpOrder" parameterType="com.winning.hic.model.OutpOrder">
        delete from  OUTP_ORDER
        <where>
            <if test="(xh != null and xh != '')"> and XH = #{ xh }   </if>
            <if test="(xh !=null or xh!='') and (map.pks != null and map.pks.size > 0)">
                and (XH) in
                <foreach item="item" collection="map.pks" open="(" separator="," close=")"> #{item}</foreach>
