                <if test="(jhzh != null and jhzh != '')"> JHZH = #{jhzh , jdbcType= VARCHAR }, </if>
                <if test="(bzlb != null and bzlb != '')"> BZLB = #{bzlb , jdbcType= VARCHAR }, </if>
                <if test="(bzdm != null and bzdm != '')"> BZDM = #{bzdm , jdbcType= VARCHAR }, </if>
                <if test="(bzmc != null and bzmc != '')"> BZMC = #{bzmc , jdbcType= VARCHAR }, </if>
                <if test="(cyydsl != null and cyydsl != '')"> CYYDSL = #{cyydsl , jdbcType= INTEGER }, </if>
                <if test="(kdbz != null and kdbz != '')"> KDBZ = #{kdbz , jdbcType= BIT }, </if>
                <if test="(cyxzfs != null and cyxzfs != '')"> CYXZFS = #{cyxzfs , jdbcType= INTEGER }, </if>
            </trim>
        </set>
        <where>
            <if test="(xh != null and xh != '')"> and XH = #{ xh, jdbcType= DECIMAL }   </if>
            <if test="(xh !=null or xh!='') and (map.pks != null and map.pks.size > 0 != '')">
                and (XH) in
                <foreach item="item" collection="map.pks" open="(" separator="," close=")"> #{item}</foreach>
